{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strictBindCallApply": false,
    "moduleResolution": "Node",
    "jsx": "react",
    "module": "esnext",
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  }
}
